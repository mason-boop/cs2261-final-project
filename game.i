    for (int i = 0; i < 3; i++) {

        cloud[i].width = 24;
        cloud[i].height = 24;
        cloud[i].worldRow = 60;
        cloud[i].worldCol = 240;
        cloud[i].screenRow = 0;
        cloud[i].screenCol = 0;
        cloud[i].active = 0;
    }
}


void spawnCloud() {


    for (int i = 0; i < 3; i++) {

        if (!cloud[i].active) {


            cloud[i].worldRow = cloud[i].screenRow = rand() % (160 - cloud[i].height);
            cloud[i].worldCol = cloud[i].screenCol = (hOff + 240);


            cloud[i].active = 1;


            break;
        }
    }
}


void updateCloud(ANISPRITE* c) {


    if (cloudCounter >= spawnCloudTime) {

        spawnCloud();
        cloudCounter = 0;
        spawnCloudTime = (rand() % 10) + 30;
    }

    if (c->active) {


        if (collision(player.screenRow, player.screenCol, player.height, player.width,
            c->screenRow + 2, c->screenCol + 2, c->height, c->width)
            && up == 0
            && cheat == 0) {

            lives = 0;
        }


        if (c->screenCol < -c->width - 1) {

            c->active = 0;
        }


        c->screenCol = c->worldCol - hOff;
    }
}


void drawCloud() {

    for (int i = 0; i < 3; i++) {

        if (cloud[i].active) {


            shadowOAM[i + 1].attr0 = (0xFF & cloud[i].screenRow) | (0<<13) | (0<<14);
            shadowOAM[i + 1].attr1 = (0x1FF & cloud[i].screenCol) | (2<<14);
            shadowOAM[i + 1].attr2 = ((0)*32+(4)) | ((0)<<12);
        } else {

            shadowOAM[i + 1].attr0 = (2<<8);
        }
    }
}


void initSpike() {

    for (int i = 0; i < 2; i++) {

        spike[i].width = 24;
        spike[i].height = 24;
        spike[i].worldRow = 60;
        spike[i].worldCol = 240;
        spike[i].screenRow = 0;
        spike[i].screenCol = 0;
        spike[i].active = 0;
    }
}


void spawnSpike() {


    for (int i = 0; i < 2; i++) {

        if (!spike[i].active) {


            spike[i].worldRow = spike[i].screenRow = rand() % (160 - spike[i].height);
            spike[i].worldCol = spike[i].screenCol = (hOff + 240) + rand() % 240;


            spike[i].active = 1;


            break;
        }
    }
}


void updateSpike(ANISPRITE* s) {


    if (spikeCounter >= spawnSpikeTime) {
        spawnSpike();
        spikeCounter = 0;
        spawnSpikeTime = (rand() % 25) + 175;
    }

    if (s->active) {


        if (s->screenRow <= 0 && direction == -2) {

            direction = 2;
        }


        if (s->screenRow + s->height >= 160 && direction == 2) {

            direction = -2;
        }


        if (collision(player.screenRow, player.screenCol, player.height, player.width,
            s->screenRow, s->screenCol, s->height, s->width)
            && up == 0
            && cheat == 0) {

            lives = 0;
        }


        s->screenCol = s->worldCol - hOff;


        s->screenRow += direction;


        if (s->screenCol < -s->width - 1) {

            s->active = 0;
        }
    }
}


void drawSpike() {

    for (int i = 0; i < 2; i++) {

        if (spike[i].active) {


            shadowOAM[i + 4].attr0 = (0xFF & spike[i].screenRow) | (0<<13) | (0<<14);
            shadowOAM[i + 4].attr1 = (0x1FF & spike[i].screenCol) | (2<<14);
            shadowOAM[i + 4].attr2 = ((0)*32+(8)) | ((0)<<12);

        } else {

            shadowOAM[i + 4].attr0 = (2<<8);
        }
    }
}


void initIK() {

    iceking.worldRow = 60;
    iceking.worldCol = 240;
    iceking.screenRow = 0;
    iceking.screenCol = 0;
    iceking.height = 32;
    iceking.width = 48;
    iceking.cdel = 2;
    iceking.active = 0;
    iceking.aniState = IK1;
    iceking.aniCounter = 0;
    iceking.curFrame = 0;
    iceking.numFrames = 3;
    iceking.bulletTimer = 0;
}


void spawnIK() {


    if (!iceking.active) {


        iceking.worldRow = rand() % (160 - iceking.height);
        iceking.worldCol = (hOff + 240) + rand() % 240;


        iceking.screenRow = iceking.worldRow - vOff;
        iceking.screenCol = iceking.worldCol - hOff;


        iceking.active = 1;
    }
}


void updateIK() {


    if (icekingCounter >= spawnIcekingTime) {

        spawnIK();
        icekingCounter = 0;
        spawnIcekingTime = (rand() % 80) + 300;
    }

    if (iceking.active) {


        if (collision(player.screenRow, player.screenCol, player.height, player.width,
            iceking.screenRow + 5, iceking.screenCol, iceking.height, iceking.width)
            && up == 0
            && cheat == 0) {

            lives = 0;
        }


        iceking.worldCol--;


        if (iceking.aniCounter % 36 < 12) {

            iceking.curFrame = 0;
        } else if (iceking.aniCounter % 36 < 24) {

            iceking.curFrame = 1;
        } else {

            iceking.curFrame = 2;
        }

        iceking.aniCounter++;


        iceking.screenRow = iceking.worldRow - vOff;
        iceking.screenCol = iceking.worldCol - hOff;


        if (iceking.screenCol < -iceking.width - 1) {

            iceking.active = 0;
        }


        if (iceking.bulletTimer > 8 && up == 0) {

            fireIKBullet();
            iceking.bulletTimer = 0;
        }

        iceking.bulletTimer++;
    }
}


void drawIK() {

    if (iceking.active) {


        shadowOAM[6].attr0 = (0xFF & iceking.screenRow) | (0<<13) | (1<<14);
        shadowOAM[6].attr1 = (0x1FF & iceking.screenCol) | (3<<14);
        shadowOAM[6].attr2 = ((iceking.curFrame * 4)*32+(12)) | ((0)<<12);

    } else {

        shadowOAM[6].attr0 = (2<<8);
    }
}


void initIKBullets() {

    for (int i = 0; i < 2; i++) {

        ikBullets[i].height = 5;
        ikBullets[i].width = 16;
        ikBullets[i].worldRow = -ikBullets[i].height * 2;
        ikBullets[i].worldCol = -ikBullets[i].width * 2;
        ikBullets[i].screenRow = ikBullets[i].worldRow;
        ikBullets[i].screenCol = ikBullets[i].worldCol;
        ikBullets[i].cdel = 2;
        ikBullets[i].active = 0;

    }
}


void fireIKBullet() {


    for (int i = 0; i < 2; i++) {

        if (!ikBullets[i].active) {


            ikBullets[i].worldRow = iceking.worldRow + (iceking.height / 2);
            ikBullets[i].worldCol = iceking.worldCol;

            ikBullets[i].screenRow = ikBullets[i].worldRow - vOff;
            ikBullets[i].screenCol = ikBullets[i].worldCol - hOff;


            ikBullets[i].active = 1;


            break;
        }
    }
}


void updateIKBullet(ANISPRITE* b) {


    if (b->active) {

        if (b->screenCol + b->width - 1 >= 0) {

            b->worldCol -= b->cdel;
        } else {

            b->active = 0;
        }


        if (collision(player.worldRow, player.worldCol, player.height, player.width - 8,
            b->worldRow, b->worldCol, b->height, b->width)
            && up == 0
            && cheat == 0) {

            lives = 0;
        }


        b->screenRow = b->worldRow - vOff;
        b->screenCol = b->worldCol - hOff;
    }
}


void drawIKBullet(ANISPRITE* b) {

    if(b->active) {

        shadowOAM[7].attr0 = (0xFF & b->screenRow) | (0<<13) | (0<<14);
        shadowOAM[7].attr1 = (0x1FF & b->screenCol) | (1<<14);
        shadowOAM[7].attr2 = ((2)*32+(20)) | ((0)<<12);

    } else {

        shadowOAM[7].attr0 = (2<<8);
    }
}


