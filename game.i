void initGame() {


    hideSprites();


    initPlayer();
    initCloud();
    initSpike();
    initIK();
    initIKBullets();
    initUp();


    hOff = 0;
    vOff = 0;


    lives = 1;
    direction = 2;
    up = 0;
    upTimer = 0;
    cheat = 0;
    score = 0;


    cloudCounter = 0;
    spikeCounter = 0;
    icekingCounter = 0;
    powerupCounter = 0;


    spawnCloudTime = (rand() % 10) + 30;
    spawnSpikeTime = (rand() % 25) + 175;
    spawnIcekingTime = (rand() % 80) + 300;
    spawnPowerupTime = (rand() % 300) + 800;
}


void updateGame() {


    if ((up == 1 && upTimer <= 300) || cheat == 1) {

        hOff += 6;
    } else {

        hOff += 2;
        up = 0;
        upTimer = 0;
        cheat = 0;
    }


    updatePlayer();


    for (int i = 0; i < 3; i++) {
        updateCloud(&cloud[i]);
    }


    for (int i = 0; i < 2; i++) {
        updateSpike(&spike[i]);
    }


    updateIK();


    for (int i = 0; i < 2; i++) {
        updateIKBullet(&ikBullets[i]);
    }


    updateUp();


    cloudCounter++;
    spikeCounter++;
    icekingCounter++;
    powerupCounter++;
    upTimer++;


    score++;
}


void drawGame() {


    drawScore();


    drawPlayer();


    for (int i = 0; i < 2; i++) {
        drawIKBullet(&ikBullets[i]);
    }


    drawCloud();
    drawSpike();
    drawIK();
    drawUp();


    (*(volatile unsigned short *)0x04000010) = hOff + (hOff / 2);
    (*(volatile unsigned short *)0x04000014) = hOff - (hOff / 2);
}


void drawScore() {


    int thousands = score / 1000;
    int hundreds = (score - (thousands * 1000)) / 100;
    int tens = (score - (thousands * 1000) - (hundreds * 100)) / 10;
    int ones = (score - (thousands * 1000) - (hundreds * 100) - (tens * 10));


    for (int i = 0; i < 4; i++) {

        shadowOAM[i + 10].attr0 = (0xFF & 6) | (0<<13) | (0<<14);
        shadowOAM[i + 10].attr1 = (0x1FF & ((95 + (i * 12)))) | (1<<14);
    }

    shadowOAM[10].attr2 = ((thousands * 2)*32+(25)) | ((0)<<12);
    shadowOAM[11].attr2 = ((hundreds * 2)*32+(25)) | ((0)<<12);
    shadowOAM[12].attr2 = ((tens * 2)*32+(25)) | ((0)<<12);
    shadowOAM[13].attr2 = ((ones * 2)*32+(25)) | ((0)<<12);
}


void initPlayer() {

    player.worldRow = (160 / 2) - (player.width / 2);
    player.worldCol = 10;
    player.screenRow = 0;
    player.screenCol = 0;
    player.height = 25;
    player.width = 24;
    player.rdel = 3;
    player.aniState = PLAYER1;
    player.aniCounter = 0;
    player.curFrame = 0;
    player.numFrames = 2;
    player.bulletTimer = 20;
}


void updatePlayer() {


    if ((~((*(volatile unsigned short *)0x04000130)) & ((1<<6))) && player.worldRow >= player.rdel) {

        player.worldRow -= player.rdel;

    } else if ((~((*(volatile unsigned short *)0x04000130)) & ((1<<7))) && player.worldRow + player.height + 5 + player.rdel <= 160) {

        player.worldRow += player.rdel;
    }


    if (up == 1 || cheat == 1) {

        player.worldCol += 6;
    } else {

        player.worldCol += 2;


        if (player.aniCounter % 30 < 15) {

            player.curFrame = 0;
        } else {

            player.curFrame = 1;
        }

        player.aniCounter++;
    }


    if ((~((*(volatile unsigned short *)0x04000130)) & ((1<<0)))) {

        cheat = 1;
    } else {

        cheat = 0;
    }


    player.screenRow = player.worldRow - vOff;
    player.screenCol = player.worldCol - hOff;
}


void drawPlayer() {

    shadowOAM[0].attr0 = player.screenRow | (0<<13) | (0<<14);
    shadowOAM[0].attr1 = player.screenCol | (2<<14);

    if (up == 1 || cheat == 1) {

        shadowOAM[0].attr2 = ((8)*32+(0)) | ((0)<<12);
    } else {

        shadowOAM[0].attr2 = ((player.curFrame * 4)*32+(0)) | ((0)<<12);
    }
}


